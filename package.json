{
  "name": "percent-change",
  "version": "0.1.0",
  "description": "Calculate the change in percentage for two numbers",
  "main": "index.js",
  "dependencies": {
    "nopt": "~3.0.1"
  },
  "devDependencies": {
    "jostle": "~1.0.0",
    "blanket": "~1.1.6",
    "mocha": "~1.20.1",
    "should": "~4.0.4"
  },
  "scripts": {
    "test": "mocha -R spec",
    "cover": "mocha -r blanket -R json-cov | jostle",
    "cover-html": "mocha -r blanket -R html-cov > coverage.html",
    "circle": "mkdir -p $CIRCLE_ARTIFACTS && mocha -r blanket -R html-cov > $CIRCLE_ARTIFACTS/coverage.html"
  },
  "config": {
    "blanket": {
      "pattern": "/lib/",
      "data-cover-never": "node_modules"
    }
  },
  "bin": {
    "percent-change": "./bin/cmd.js"
  },
  "license": "MIT",
  "keywords": [
    "percent",
    "percentage",
    "change"
  ],
  "author": "Evan Lucas <evanlucas@me.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/evanlucas/percent-change"
  },
  "homepage": "https://github.com/evanlucas/percent-change",
  "bugs": {
    "url": "https://github.com/evanlucas/percent-change/issues"
  }
}
